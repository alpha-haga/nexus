{
  "realm": "nexus",
  "enabled": true,
  "registrationAllowed": false,
  "loginWithEmailAllowed": true,
  "duplicateEmailsAllowed": false,
  "sslRequired": "external",
  "clientScopes": [
    {
      "name": "nexus-db-access",
      "description": "NEXUS DB Access Scope - client roles を nexus_db_access claim として出力",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "false"
      },
      "protocolMappers": [
        {
          "name": "nexus-db-access-mapper",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-client-role-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "true",
            "userinfo.token.claim": "false",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "nexus_db_access",
            "jsonType.label": "String",
            "usermodel.clientRoleMapping.clientId": "nexus-bff"
          }
        }
      ]
    },
    {
      "name": "profile",
      "description": "Default profile scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "profile"
      },
      "protocolMappers": [
        {
          "name": "preferred-username",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "false",
            "userinfo.token.claim": "false",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "preferred_username",
            "jsonType.label": "String",
            "user.attribute": "username"
          }
        }
      ]
    },
    {
      "name": "email",
      "description": "Default email scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "email"
      },
      "protocolMappers": [
        {
          "name": "email",
          "protocol": "openid-connect",
          "protocolMapper": "oidc-usermodel-property-mapper",
          "consentRequired": false,
          "config": {
            "multivalued": "false",
            "userinfo.token.claim": "false",
            "id.token.claim": "true",
            "access.token.claim": "true",
            "claim.name": "email",
            "jsonType.label": "String",
            "user.attribute": "email"
          }
        }
      ]
    },
    {
      "name": "roles",
      "description": "Default roles scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "roles"
      }
    },
    {
      "name": "web-origins",
      "description": "Default web origins scope",
      "protocol": "openid-connect",
      "attributes": {
        "include.in.token.scope": "true",
        "display.on.consent.screen": "true",
        "consent.screen.text": "web-origins"
      }
    }
  ],
  "clients": [
    {
      "clientId": "nexus-frontend",
      "enabled": true,
      "publicClient": false,
      "secret": "nexus-frontend-secret",
      "redirectUris": [
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "http://localhost:3000"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "protocol": "openid-connect",
      "attributes": {
        "post.logout.redirect.uris": "http://localhost:3000/*"
      },
      "defaultClientScopes": [
        "profile",
        "email",
        "roles",
        "web-origins",
        "nexus-db-access"
      ]
    },
    {
      "clientId": "nexus-bff",
      "name": "NEXUS BFF Client",
      "description": "Backend for Frontend - DB アクセス権限を client role で管理",
      "enabled": true,
      "publicClient": false,
      "secret": "nexus-bff-secret",
      "redirectUris": [
        "http://localhost:8080/*",
        "http://localhost:3000/*"
      ],
      "webOrigins": [
        "http://localhost:8080",
        "http://localhost:3000"
      ],
      "standardFlowEnabled": true,
      "directAccessGrantsEnabled": true,
      "serviceAccountsEnabled": false,
      "protocol": "openid-connect",
      "attributes": {
        "post.logout.redirect.uris": "http://localhost:8080/*"
      },
      "defaultClientScopes": [
        "profile",
        "email",
        "nexus-db-access"
      ]
    }
  ],
